{
  "name": "next-app-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "prisma migrate deploy && next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "format.check": "prettier --check .",
    "format.fix": "prettier --write .",
    "check.types": "tsc",
    "code.check.types": "tsc --noEmit",
    "codegen.rm": "rm -f types/gql/*",
    "codegen.generate": "graphql-codegen",
    "codegen.watch": "graphql-codegen --watch",
    "codegen": "npm-run-all codegen.rm codegen.generate",
    "codegen.prisma": "prisma generate",
    "db.migrations.dev": "prisma migrate dev",
    "db.migrations.deploy": "prisma migrate deploy",
    "db.reset": "prisma migrate reset -f",
    "db.studio": "prisma studio",
    "test": "yarn jest"
  },
  "dependencies": {
    "@apollo/client": "^3.8.0-alpha.15",
    "@apollo/experimental-nextjs-app-support": "^0.1.0",
    "@apollo/server": "^4.7.2",
    "@as-integrations/next": "^2.0.0",
    "@prisma/client": "^4.15.0",
    "@types/node": "20.2.5",
    "@types/react": "18.2.8",
    "@types/react-dom": "18.2.4",
    "dataloader": "^2.2.2",
    "deepmerge": "^4.3.1",
    "eslint": "8.42.0",
    "eslint-config-next": "^13.4.4",
    "graphql": "^16.6.0",
    "graphql-scalars": "^1.22.2",
    "graphql-tag": "^2.12.6",
    "i18n-js": "^4.2.3",
    "next": "13.4.4",
    "nexus": "^1.3.0",
    "prisma": "^4.15.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "styled-components": "^5.3.11",
    "typescript": "5.1.3"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^4.0.1",
    "@graphql-codegen/client-preset": "^4.0.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@types/styled-components": "^5.1.26",
    "eslint-config-prettier": "^8.8.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8"
  }, 
  "resolutions": {
    "styled-components": "^5",
    "@types/react": "^18.0.0"
  },
}
