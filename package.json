{
  "name": "next-app-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "prisma migrate deploy && next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "format.check": "prettier --check .",
    "format.fix": "prettier --write .",
    "check.types": "tsc",
    "code.check.types": "tsc --noEmit",
    "codegen.rm": "rm -f types/gql/*",
    "codegen.generate": "graphql-codegen",
    "codegen.watch": "graphql-codegen --watch",
    "codegen": "npm-run-all codegen.rm codegen.generate",
    "codegen.prisma": "prisma generate",
    "db.migrations.dev": "prisma migrate dev",
    "db.migrations.deploy": "prisma migrate deploy",
    "db.reset": "prisma migrate reset -f",
    "db.studio": "prisma studio",
    "test": "yarn jest"
  },
  "dependencies": {
    "@apollo/client": "^3.8.7",
    "@apollo/experimental-nextjs-app-support": "^0.5.1",
    "@apollo/server": "^4.9.5",
    "@as-integrations/next": "^3.0.0",
    "@mdi/js": "^7.3.67",
    "@mdi/react": "^1.6.1",
    "@prisma/client": "^5.6.0",
    "@types/node": "20.9.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "18.2.15",
    "dataloader": "^2.2.2",
    "date-fns": "^2.30.0",
    "deepmerge": "^4.3.1",
    "eslint": "8.53.0",
    "eslint-config-next": "^14.0.2",
    "framer-motion": "^10.16.5",
    "graphql": "^16.8.1",
    "graphql-scalars": "^1.22.4",
    "graphql-tag": "^2.12.6",
    "i18n-js": "^4.3.2",
    "lottie-react": "^2.4.0",
    "next": "14.0.2",
    "nexus": "^1.3.0",
    "prisma": "^5.6.0",
    "react": "18.2.0",
    "react-calendar": "^4.6.1",
    "react-date-picker": "^10.5.2",
    "react-dom": "18.2.0",
    "styled-components": "^6.1.1",
    "typescript": "5.2.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/client-preset": "^4.1.0",
    "@testing-library/jest-dom": "^6.1.4",
    "@types/styled-components": "^5.1.30",
    "eslint-config-prettier": "^9.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.0"
  },
  "resolutions": {
    "styled-components": "^5",
    "@types/react": "^18.0.0"
  }
}
